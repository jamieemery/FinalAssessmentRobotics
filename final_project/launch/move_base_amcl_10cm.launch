<launch>
  <master auto="start"/>
  <param name="/use_sim_time" value="true"/>
  <include file="$(find final_project)/move_base_config/move_base.xml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(find final_project)/world/lgfloor.pgm 0.05" respawn="false" />
  <node pkg="stage_ros" type="stageros" name="stageros" args="$(find final_project)/world/lgfloor.world" respawn="false">
    <param name="base_watchdog_timeout" value="0.2"/>
  </node>
  <include file="$(find final_project)/move_base_config/amcl_node.xml"/>
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find final_project)/single_robot.rviz" /> 
</launch>
